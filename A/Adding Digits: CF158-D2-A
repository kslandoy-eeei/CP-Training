#include <bits/stdc++.h>
using namespace std;

int main()
{
    int a, b, n;
    bool done = false;

    cin >> a >> b >> n;

    for (int i = 0; i < 10; i++)
    {
        if ((a * 10 + i) % b == 0)
        {
            cout << (a * 10 + i); 
            done = true;
            break;
        }
    }

    if (!done)
    {
        cout << -1;
    }

    else
    {
        for (int i = 0; i < n - 1; i++)
        {
            cout << 0;
        }
    }
}
