void backtrack(int S, length = 6, string game = "")
{
    if (!length)
    {
        cout << game;
    }
}

int main()
{
    while (1)
    {
        int k;
        int nums[6];

        cin >> k;

        for (int i = 0; i < k; i++)
        {
            cin >> nums[i];
        }


    }
}